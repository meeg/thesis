  urldate = {2016-09-30},
  date = {2011},
  pages = {032012},
  file = {IOP Full Text PDF:/home/meeg/.zotero/zotero/npn5ytqx.default/zotero/storage/CAQ4VCRF/Graf - 2011 - org.lcsim Event Reconstruction in Java.pdf:application/pdf}
