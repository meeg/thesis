  file = {IOP Full Text PDF:/home/meeg/.zotero/zotero/npn5ytqx.default/zotero/storage/GBSGJWCW/Tyson et al. - 1998 - Detailed Mass Map of CL 0024+1654 from Strong Lens.pdf:application/pdf}
}

